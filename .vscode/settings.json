{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/dist": true,
    "**/coverage": true
  },
  "typescript.referencesCodeLens.enabled": true,
  "git.ignoreLimitWarning": true,
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#77428f",
    "activityBar.activeBorder": "#f6f7f8",
    "activityBar.background": "#4e2262",
    "activityBar.foreground": "#f8b133",
    "activityBar.inactiveForeground": "#f6f7f8",
    "activityBarBadge.background": "#f8b133",
    "activityBarBadge.foreground": "#000000",
    "statusBar.background": "#4e2262",
    "statusBar.foreground": "#f6f7f8"
  }
}
